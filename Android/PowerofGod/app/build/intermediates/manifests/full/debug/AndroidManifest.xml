<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="apdevelopmenta.powerofgod"
    android:versionCode="6"
    android:versionName="Alpha 1.4" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="apdevelopment.powerofgod.alpha.MainScreen.MainScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="apdevelopment.powerofgod.alpha.PrivacyPolicyActivity"
            android:label="Privacy Policy" >
        </activity>
        <activity
            android:name="apdevelopment.powerofgod.alpha.User.Settings.SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
    </application>

</manifest>